	section Medical_Staffs
	{
		displayname= "Medical Staffs"
		icon = "health-notebook"
		form Medical_Staff
		{
			displayname = "Medical Staff"
			success message = "Medical Staff Added Successfully"
			field alignment = left
			Section
			(
				type = section
	 			row = 1
	 			column = 0   
				width = medium
			)
			must have Full_Name
			(
				type = picklist
				displayname = "Full Name"
				values = {"Dr. Russel F.", "Dr. Job E.", "Dra. Pia E.", "Dr. Edison P.", "Dr. Natchi S."}
				sortorder = ascending
				others option = true
	 			row = 1
	 			column = 1   
				width = medium
			)
			Specialization
			(
				type = picklist
				values = {"Eurology", "Obstetrician ", "Pediatrician"}
				sortorder = ascending
				others option = true
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have hospital_and_organization
			(
				type = picklist
				displayname = "Hospital and Organization"
				values = {"Manila doctors", "Asian Hospital", "St Luke", "Makati Medical Center"}
				sortorder = ascending
				others option = true
    			description
    			[
    				type = help_text
    			 	message = "Select accreditted Doctors and Hospitals"
    			]
	 			row = 1
	 			column = 1   
				width = medium
			)
			Address
			(
    			type = address
     			capture_coordinates = true
     			adjust_using_map = true
     			address_line_1
     			(
	     			  type = address_line_1
	     			  displayname = "Address Line 1"
     			) 
     			address_line_2
     			(
	     			  type = address_line_2
	     			  displayname = "Address Line 2"
	     			 visibility = false
     			) 
     			district_city
     			(
	     			  type = district_city
	     			  displayname = "City / District"
     			) 
     			state_province
     			(
	     			  type = state_province
	     			  displayname = "State / Province"
     			) 
     			postal_Code
     			(
	     			  type = postal_Code
	     			  displayname = "Postal Code"
     			) 
